{
  "name": "gatsby-starter-advanced",
  "description": "GatsbyJS starter that includes examples for advanced use cases.",
  "version": "1.1.0",
  "author": "Ruben Harutyunyan <vagr9k@gmail.com>",
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.6",
    "gatsby": "^2.13.32",
    "gatsby-image": "^2.2.29",
    "gatsby-plugin-catch-links": "^2.1.2",
    "gatsby-plugin-feed": "^2.3.5",
    "gatsby-plugin-google-analytics": "^2.1.4",
    "gatsby-plugin-lodash": "^3.1.2",
    "gatsby-plugin-manifest": "^2.2.3",
    "gatsby-plugin-nprogress": "^2.1.2",
    "gatsby-plugin-offline": "^2.2.4",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.2",
    "gatsby-plugin-react-helmet": "^3.1.2",
    "gatsby-plugin-sharp": "^2.2.8",
    "gatsby-plugin-sitemap": "^2.2.3",
    "gatsby-plugin-styled-components": "^3.1.2",
    "gatsby-plugin-twitter": "^2.1.2",
    "gatsby-remark-autolink-headers": "^2.1.3",
    "gatsby-remark-copy-linked-files": "^2.1.3",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.1.28",
    "gatsby-remark-prismjs": "^3.3.3",
    "gatsby-remark-responsive-iframe": "^2.2.4",
    "gatsby-source-filesystem": "^2.1.5",
    "gatsby-transformer-remark": "^2.6.8",
    "gatsby-transformer-sharp": "^2.2.4",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prismjs": "^1.17.1",
    "react": "^16.8.6",
    "react-accessible-accordion": "^3.0.0",
    "react-adsense": "^0.1.0",
    "react-disqus-comments": "^1.4.0",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-share": "^2.4.0",
    "react-twitter-widgets": "^1.7.1",
    "styled-components": "^4.3.2",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "cli-glob": "^0.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "gh-pages": "^2.0.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "mediumexporter": "^0.1.6",
    "prettier": "^1.19.1",
    "remark-cli": "^6.0.1",
    "remark-preset-lint-recommended": "^3.0.3",
    "stylefmt": "^6.0.3",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "write-good": "^1.0.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf public && rm -rf .cache",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:md": "remark content/posts/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "medium:import": "mediumexporter"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.css": [
      "stylelint --fix",
      "git add"
    ]
  }
}
